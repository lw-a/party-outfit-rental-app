<div class="container my-4">
  <br>
  <div class="row justify-content-center">
    <div class="col-12 col-lg-8">
      <h2>My bookings</h2>
      <br>
      <% if @bookings.empty? %>
        <p>No bookings yet...</p>
      <% else %>
        <% @bookings.each do |booking| %>
          <%= render 'card_booking', booking: booking, outfit: booking.outfit %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
