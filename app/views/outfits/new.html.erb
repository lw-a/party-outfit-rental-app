<%= simple_form_for(@user, @outfit) do |f| %>
  <%= f.input = :name %>
  <%= f.input = :size %>
  <%= f.input = :color %>
  <%= f.input = :description %>
  <%= f.input = :category %>
  <%= f.input = :price %>
  <%= f.input = :location %>
<% end %>

<button type="button" class="mt-2 btn btn-dark"><%= link_to "←", outfits_path %></button>
