<h2><strong>Book this outfit!</strong></h2>
<%= simple_form_for [@outfit, @booking] do |f| %>
  <%= f.input :start_date, as: :date, html5: true %>
  <%= f.input :end_date, as: :date, html5: true %>
  <%= f.input :comment %>
  <%= f.submit 'Book outfit', class: 'btn btn-primary btn-block' %>
<% end %>
